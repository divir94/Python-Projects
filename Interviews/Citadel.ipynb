    "def str_to_int(inp):\n",
    "    n = len(inp)\n",
    "    return sum((ord(d) - ord('0'))*(10**(n - i - 1)) for i, d in enumerate(inp))\n",
    "\n",
    "print str_to_int('1673')"
   ]
